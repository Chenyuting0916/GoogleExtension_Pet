{
  "name": "GoogleExtension_Pet",
  "version": "1.0.0",
  "description": "Developed by FBI & ODOG!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "web_accessible_resources": [
    "popup.html",
    "pet_image/*.png",
    "pet_image/*.jpg",
    "fontawesome-free-5.13.0-web/webfonts/*",
    "fontawesome-free-5.13.0-web/css/*",
    "fontawesome-free-5.13.0-web/js/*",
    "fontawesome-free-5.13.0-web/less/*",
    "fontawesome-free-5.13.0-web/metadata/*",
    "fontawesome-free-5.13.0-web/scss/*",
    "fontawesome-free-5.13.0-web/sprites/*",
    "fontawesome-free-5.13.0-web/svgs/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "css": [
        "fontawesome-free-5.13.0-web/css/all.css"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "pet.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery-3.4.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "pet",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/pet16.png",
      "32": "icon/pet32.png",
      "48": "icon/pet48.png",
      "128": "icon/pet128.png"
    }
  },
  "icons": {
    "16": "icon/pet16.png",
    "32": "icon/pet32.png",
    "48": "icon/pet48.png",
    "128": "icon/pet128.png"
  },
  "manifest_version": 2
}